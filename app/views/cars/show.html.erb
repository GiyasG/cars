    <div class='row'>
      <%= content_tag :h4, "#{@car.make}", class: "col-md-12"%>
      <%= content_tag :h4, "#{@car.model}", class: "col-md-12"%>
      <%= image_tag url_for(:controller => "photos", :action => "show_car", :car_id=>@car.id), class: "col-md-4 img-thumbnail"%>

      <%= content_tag :h4, "Make year:", class: "col-md-4" do%>
        <%= content_tag :span, "#{@car.year.strftime("%Y")}"%>
      <% end%>
      <%= content_tag :h4, "Version: #{@car.version}", class: "col-md-4" %>
      <%= content_tag :h4, "Classification: #{@car.classification}", class: "col-md-4" %>
      <%= content_tag :h4, "Body type: #{@car.bodytype}", class: "col-md-4" %>
      <%= content_tag :h4, "Doors: #{@car.door}", class: "col-md-4" %>
      <%= content_tag :h4, "Seats: #{@car.seat}", class: "col-md-4" %>
      <%= content_tag :h4, "Engine place: #{@car.engineplace}", class: "col-md-4" %>
      <%= content_tag :h4, "Drivetrain: #{@car.drivetrain}", class: "col-md-4" %>

    </div>
