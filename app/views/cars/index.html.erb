<p id="notice"><%= notice %></p>

<h1>Listing Cars</h1>

<table>
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Year</th>
      <th>Body</th>
      <th>Version</th>
      <th>Classification</th>
      <th>Type</th>
      <th>Door</th>
      <th>Seat</th>
      <th>Engineplace</th>
      <th>Drivetrain</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.make %></td>
        <td><%= car.model %></td>
        <td><%= car.year %></td>
        <td><%= car.body %></td>
        <td><%= car.version %></td>
        <td><%= car.classification %></td>
        <td><%= car.type %></td>
        <td><%= car.door %></td>
        <td><%= car.seat %></td>
        <td><%= car.engineplace %></td>
        <td><%= car.drivetrain %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
